{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind"
      ]
    }],
    "block-no-empty": false,
    "property-no-unknown": false,
    "unit-no-unknown": [true, {
      "ignoreUnits": ["/.*/"]
    }],
  }
}