{
  "name": "deerpark.github.io",
  "version": "0.1.0",
  "private": true,
  "author": "Yonn Kim",
  "contributors": [
    {
      "name": "Yonn Kim",
      "email": "deerpark8210@gmail.com",
      "url": "http://deerpark.github.io/"
    }
  ],
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deerpark/deerpark.github.io.git"
  },
  "homepage": "http://deerpark.github.io",
  "eslintConfig": {
    "extends": [
      "airbnb",
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "semi": 0,
      "comma-dangle": 0,
      "consistent-return": 0,
      "global-require": 0,
      "@typescript-eslint/semi": 0,
      "@typescript-eslint/comma-dangle": 0,
      "eslint-disable-next-line": 0,
      "jsx-a11y/control-has-associated-label": 0,
      "react/button-has-type": 0,
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "react/forbid-prop-types": 0,
      "react/no-children-prop": 0,
      "react/jsx-no-target-blank": 0,
      "react-hooks/exhaustive-deps": 0,
      "react/jsx-props-no-spreading": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.33-beta1",
    "@fortawesome/pro-duotone-svg-icons": "^6.0.0-beta1",
    "@fortawesome/pro-light-svg-icons": "^6.0.0-beta1",
    "@fortawesome/pro-regular-svg-icons": "^6.0.0-beta1",
    "@fortawesome/pro-solid-svg-icons": "^6.0.0-beta1",
    "@fortawesome/pro-thin-svg-icons": "^6.0.0-beta1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@juggle/resize-observer": "^3.3.1",
    "@react-spring/web": "^9.2.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@use-gesture/react": "^10.0.0-beta.9",
    "autoprefixer": "9",
    "classnames": "^2.3.1",
    "framer-motion": "^4.1.17",
    "node-sass": "5.0.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-anime": "^4.0.4",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.32.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scrollbars-custom": "^4.0.25",
    "react-use-measure": "^2.0.4",
    "recoil": "^0.3.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^7",
    "prettier": "^2.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.css": [
      "stylelint",
      "git add"
    ],
    "src/**/*": [
      "prettier --write",
      "git add"
    ]
  }
}
